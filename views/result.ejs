<div>
    <h2> Search Results </h2>
    
    <div class = "container">
        <ul>
            <% data.forEach(post => { %>
                <li>
                    <a href = "/description/<%= post._id %>">
                        <span> <%= post.comictitle %></span>
                    </a>
                </li>
            <% }) %>
        </ul>
    </div>
</div>